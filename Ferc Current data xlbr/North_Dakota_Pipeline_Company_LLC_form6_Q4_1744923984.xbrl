  <ferc:StatementAboutImportantChangesDuringTheQuarterYearTextBlock id="f-8" contextRef="c-1">&lt;div style="padding-left:18pt"&gt;&lt;span style="color:#000000;font-family:'Arial',sans-serif;font-size:10pt;font-weight:400;line-height:104%"&gt;1. None&lt;/span&gt;&lt;/div&gt;&lt;div style="padding-left:18pt"&gt;&lt;span style="color:#000000;font-family:'Arial',sans-serif;font-size:10pt;font-weight:400;line-height:104%"&gt;2. None&lt;/span&gt;&lt;/div&gt;&lt;div style="padding-left:18pt"&gt;&lt;span style="color:#000000;font-family:'Arial',sans-serif;font-size:10pt;font-weight:400;line-height:104%"&gt;3. None&lt;/span&gt;&lt;/div&gt;&lt;div style="padding-left:18pt"&gt;&lt;span style="color:#000000;font-family:'Arial',sans-serif;font-size:10pt;font-weight:400;line-height:104%"&gt;4. None&lt;/span&gt;&lt;/div&gt;&lt;div style="padding-left:18pt"&gt;&lt;span style="color:#000000;font-family:'Arial',sans-serif;font-size:10pt;font-weight:400;line-height:104%"&gt;5. N/A&lt;/span&gt;&lt;/div&gt;</ferc:StatementAboutImportantChangesDuringTheQuarterYearTextBlock>
